{
  "extends": "tslint:recommended",
  "rules": {
    "array-type": [true, "generic"],
    "max-line-length": {
      "options": [120]
    },
    "member-access": false,
    "member-ordering": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
